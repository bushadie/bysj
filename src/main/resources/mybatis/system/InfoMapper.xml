<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bushadie.project.system.competition.mapper.InfoMapper">

    <resultMap type="cn.bushadie.project.system.competition.domain.Info" id="InfoResult">
        <result property="id" column="id"/>
        <result property="competitionid" column="competitionid"/>
        <result property="k" column="k"/>
        <result property="v" column="v"/>
    </resultMap>

    <sql id="selectInfoVo">
        select id, competitionid, k, v
        from com_info
    </sql>

    <select id="selectInfoList" parameterType="cn.bushadie.project.system.competition.domain.Info"
            resultMap="InfoResult">
        <include refid="selectInfoVo"/>
        <where>
            <if test="id != null ">and id = #{id}
            </if>
            <if test="competitionid != null ">and competitionid = #{competitionid}
            </if>
            <if test="k != null  and k != '' ">and k = #{k}
            </if>
            <if test="v != null  and v != '' ">and v = #{v}
            </if>
        </where>
    </select>

    <select id="selectInfoById" parameterType="Integer" resultMap="InfoResult">
        <include refid="selectInfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertInfo" parameterType="cn.bushadie.project.system.competition.domain.Info"  keyProperty="id" useGeneratedKeys="true">
        insert into com_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null  ">id,
            </if>
            <if test="competitionid != null  ">competitionid,
            </if>
            <if test="k != null  and k != ''  ">k,
            </if>
            <if test="v != null  and v != ''  ">v,
            </if>
        </trim>
        <trim prefix="value (" suffix=")" suffixOverrides=",">
            <if test="id != null  ">#{id},
            </if>
            <if test="competitionid != null  ">#{competitionid},
            </if>
            <if test="k != null  and k != ''  ">#{k},
            </if>
            <if test="v != null  and v != ''  ">#{v},
            </if>
        </trim>
    </insert>

    <update id="updateInfo" parameterType="cn.bushadie.project.system.competition.domain.Info">
        update com_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="competitionid != null  ">competitionid = #{competitionid},
            </if>
            <if test="k != null  and k != ''  ">k = #{k},
            </if>
            <if test="v != null  and v != ''  ">v = #{v},
            </if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteInfoById" parameterType="Integer">
        delete
        from com_info
        where id = #{id}
    </delete>

    <delete id="deleteInfoByIds" parameterType="String">
        delete from com_info where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>